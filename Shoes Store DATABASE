CREATE TABLE TENIS (
 CodTENIS INT NOT NULL,
    PRIMARY KEY (CodTENIS),
 COR VARCHAR(15),
 MARCA VARCHAR(20),
 NUMERACAO NUMERIC,
 GENERO VARCHAR(10)
);

