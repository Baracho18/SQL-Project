CREATE TABLE CLIENTE (
 CodCLIENTE INT NOT NULL,
       PRIMARY KEY (CodCLIENTE),
 SEXO VARCHAR(10),
 TELEFONE NUMERIC,
 NOME VARCHAR(20),
 NUMERO NUMERIC,
    CEP NUMERIC
);

