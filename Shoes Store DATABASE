CREATE TABLE ESTOQUE (
 CodESTOQUE INT NOT NULL,
 PRIMARY KEY (CodESTOQUE),
 Quantidade NUMERIC,
 CodTENIS INT NOT NULL,
        FOREIGN KEY (CodTENIS) REFERENCES TENIS(CodTENIS)
);

