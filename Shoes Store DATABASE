CREATE TABLE TENIS (
 CodTENIS INT NOT NULL,
    PRIMARY KEY (CodTENIS),
 COR VARCHAR(255),
 MARCA VARCHAR(255),
 NUMERACAO NUMERIC,
 GENERO VARCHAR(255)
);

CREATE TABLE ESTOQUE (
 CodESTOQUE INT NOT NULL,
 PRIMARY KEY (CodESTOQUE),
 Quantidade NUMERIC,
 CodTENIS INT,
        FOREIGN KEY (CodTENIS) REFERENCES TENIS(CodTENIS)
);

CREATE TABLE FORNECEDOR (
 CodFORNECEDOR INT NOT NULL,
        PRIMARY KEY (CodFORNECEDOR),
 NUMERO NUMERIC,
 TELEFONE NUMERIC,
 CNPJ NUMERIC,
 CEP NUMERIC
);

CREATE TABLE FUNCIONARIO (
  CodFUNCIONARIO INT NOT NULL,
       PRIMARY KEY (CodFUNCIONARIO),
  SEXO VARCHAR(255),
  NOME VARCHAR(255),
  CPF NUMERIC
);

CREATE TABLE CLIENTE (
 CodCLIENTE INT NOT NULL,
       PRIMARY KEY (CodCLIENTE),
 SEXO VARCHAR(255),
 TELEFONE NUMERIC,
 NOME VARCHAR(255),
 NUMERO NUMERIC,
    CEP NUMERIC
);

CREATE TABLE PEDIDO (
 CodPEDIDO INT NOT NULL,
         PRIMARY KEY (CodPEDIDO),
 data DATE,
 VALOR NUMERIC,
 CodCLIENTE INT,
 CodFUNCIONARIO INT,
 CodTENIS INT,
        FOREIGN KEY (CodCLIENTE) REFERENCES CLIENTE(CodCLIENTE),
        FOREIGN KEY (CodFUNCIONARIO) REFERENCES FUNCIONARIO(CodFUNCIONARIO),
        FOREIGN KEY (CodTENIS) REFERENCES TENIS(CodTENIS)

); 
