CREATE TABLE FORNECEDOR (
 CodFORNECEDOR INT NOT NULL,
        PRIMARY KEY (CodFORNECEDOR),
 NUMERO NUMERIC,
 TELEFONE NUMERIC,
 CNPJ NUMERIC,
 CEP NUMERIC,
    CodTENIS INT NOT NULL,
            FOREIGN KEY (CodTENIS) REFERENCES TENIS(CodTENIS)
);

INSERT INTO FORNECEDOR VALUES (1, 12, 11995934671, 94550761000178, 99040340, 1);
